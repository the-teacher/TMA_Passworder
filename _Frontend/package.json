{
  "name": "tma-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite dev --host 0.0.0.0 --port 4000",
    "build:clean": "rm -rf dist",
    "build": "yarn build:clean && yarn tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "test:note": "NODE_NO_WARNINGS=1 is for suppressing the warning about deprecated `punycode` package",
    "test": "NODE_NO_WARNINGS=1 jest --config jest.config.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "storybook": "storybook dev -p 4000 --no-open",
    "build-storybook": "storybook build",
    "all-checks": "yarn format && yarn format:check && yarn lint && yarn lint:fix && yarn test"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.4.0",
    "react-router": "^7.1.5",
    "sass": "^1.84.0",
    "storybook-addon-remix-react-router": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@storybook/addon-viewport": "7.6.19",
    "@storybook/blocks": "7.6.19",
    "@storybook/icons": "^1.3.2",
    "@storybook/react": "7.6.19",
    "@storybook/react-vite": "7.6.19",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/text-encoding": "^0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^3.5.0",
    "storybook": "7.6.19",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "packageManager": "yarn@4.6.0+sha512.5383cc12567a95f1d668fbe762dfe0075c595b4bfff433be478dbbe24e05251a8e8c3eb992a986667c1d53b6c3a9c85b8398c35a960587fbd9fa3a0915406728"
}
