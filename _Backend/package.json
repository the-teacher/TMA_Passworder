{
  "name": "hamster-pass-backend",
  "packageManager": "yarn@4.7.0",
  "type": "commonjs",
  "dependencies": {
    "@the-teacher/the-router": "^0.9.6",
    "express": "^4.21.2",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4"
  },
  "scripts": {
